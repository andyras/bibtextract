journal = {Biophysical Journal},
keywords = {Bacterial Proteins,Bacterial Proteins: chemistry,Biological,Biological: chemistry,Electrons,Energy Transfer,Light-Harvesting Protein Complexes,Light-Harvesting Protein Complexes: chemistry,Molecular Dynamics Simulation,Pigments,Quantum Theory,Temperature,Time Factors},
month = feb,
number = {3},
pages = {649--60},
pmid = {22325289},
title = {{Atomistic study of the long-lived quantum coherences in the Fenna-Matthews-Olson complex.}},
