doi = {10.1063/1.4789418},
file = {:Users/andyras/Dropbox/mendeley/Pavanello et al/Pavanello et al.\_The Journal of Chemical Physics\_2013\_An accurate and linear-scaling method for calculating charge-transfer excitation e.pdf:pdf},
