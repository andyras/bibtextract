journal = {Nature},
keywords = {Chemical,Electric Conductivity,Gold,Gold: chemistry,Light,Metal Nanoparticles,Metal Nanoparticles: chemistry,Metal Nanoparticles: radiation effects,Models,Photochemistry,Photochemistry: instrumentation,Silver,Silver: chemistry,Surface Plasmon Resonance,Temperature},
