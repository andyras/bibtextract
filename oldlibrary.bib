doi = {10.1021/jp0615398},
file = {:Users/andyras/Dropbox/mendeley/Madjet, Abdurahman, Renger/Madjet, Abdurahman, Renger\_The Journal of Physical Chemistry B\_2006\_Intermolecular coulomb couplings from ab initio electrostatic potent.pdf:pdf},
