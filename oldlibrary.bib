doi = {10.1126/science.1185509},
file = {:Users/andyras/Dropbox/mendeley/Tisdale et al/Tisdale et al.\_Science\_2010\_Hot-electron transfer from semiconductor nanocrystals.pdf:pdf},
issn = {1095-9203},
journal = {Science},
month = jun,
