title = {{Between Superexchange and Hopping: An Intermediate Charge-Transfer Mechanism in Poly(A)-Poly(T) DNA Hairpins.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23402652},
year = {2013}
}
