@article{Manuscript2012,
abstract = {First-principles computer simulations can contribute to a deeper understanding of the dye/semiconductor interface lying at the heart of DSCs. Here, we present the results of simulation of dye adsorption onto TiO2 surfaces, and of their implications for the functioning of dye-sensitized solar cells. We propose an integrated strategy which combines FT-IR measurements with DFT first principles simulations to individuate the energetically favorable adsorption mode acetic acid, as a meaningful model for realistic organic dyes. Although we found a sizable variability in the relative stability of the considered adsorption modes with the model system and method, a bridged bidentate structure was found to closely match the FT-IR frequency pattern, also being calculated as the most stable adsorption mode by calculations in solution. The bridged bidentate adsorption mode was found to be the most stable binding for realistic organic dyes bearing cyanoacrylic anchoring groups, while for a rhodanine-3-acetic acid anchoring group, an undissociated monodentate adsorption mode was found to be of comparable stability. The structural differences induced by the different anchoring groups were related to the different electron injection / recombination with oxidized dye properties which were experimentally assessed for the two classes of dyes. A stronger coupling and a possibly faster electron injection was also calculated for the bridged bidentate mode. We then investigated the adsorption mode and I2 binding of prototype organic dyes. Car-Parrinello molecular dynamics and geometry optimizations were performed for two coumarin dyes differing by the length of the pi-bridge separating the donor and acceptor moieties. We related the decreasing distance of the carbonylic oxygen from the titania to an increased I2 concentration in proximity of the oxide surface, which might account for the different observed photovoltaic performances. The interplay between theory/simulation and experiments appears the key to further DSCs progress, both concerning the design of new dye sensitizers and their interaction with the semiconductor and with the solution environment and/or electrolyte upon adsorption onto the semiconductor.},
author = {Anselmi, Chiara and do Mosconi, Edoardo and Pastore, Mariachiara and Ronca, Enrico and {De Angelis}, Filippo},
doi = {10.1039/c0xx00000x},
file = {:Users/andyras/Dropbox/mendeley/Anselmi et al/Anselmi et al.\_Physical Chemistry Chemical Physics\_2012\_Adsorption of organic dyes on TiO2 surfaces in dye-sensitized solar cells Interp.pdf:pdf},
journal = {Physical Chemistry Chemical Physics},
number = {207890},
title = {{Adsorption of organic dyes on TiO2 surfaces in dye-sensitized solar cells: Interplay of theory and experiment}},
year = {2012}
}
