doi = {10.1021/jp204711c},
file = {:Users/andyras/Dropbox/mendeley/Pal, Pati/Pal, Pati\_The Journal of Physical Chemistry C\_2011\_Charge Transport in Strongly Coupled Molecular Junctions “In-Phase” and “Out-.pdf:pdf},
issn = {1932-7447},
journal = {The Journal of Physical Chemistry C},
month = sep,
number = {35},
pages = {17564--17573},
title = {{Charge Transport in Strongly Coupled Molecular Junctions: “In-Phase” and “Out-of-Phase” Contribution to Electron Tunneling}},
url = {http://pubs.acs.org/doi/abs/10.1021/jp204711c},
volume = {115},
year = {2011}
}
