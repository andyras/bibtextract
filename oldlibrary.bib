pmid = {22963520},
title = {{High-Efficiency Photoelectrocatalytic Hydrogen Generation Enabled by Palladium Quantum Dots-Sensitized TiO(2) Nanotube Arrays.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22963520},
volume = {134},
