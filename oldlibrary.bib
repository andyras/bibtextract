doi = {10.1063/1.3173827},
file = {:Users/andyras/Dropbox/mendeley/Neese, Hansen, Liakos/Neese, Hansen, Liakos\_The Journal of Chemical Physics\_2009\_Efficient and accurate approximations to the local coupled cluster singles do.pdf:pdf},
