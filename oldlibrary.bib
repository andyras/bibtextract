@article{Franco2012,
abstract = {Electronic coherence dynamics in trans-polyacetylene oligomers are considered by explicitly computing the time dependent molecular polarization from the coupled dynamics of electronic and vibrational degrees of freedom in a mean-field mixed quantum-classical approximation. The oligomers are described by the Su-Schrieffer-Heeger Hamiltonian and the effect of decoherence is incorporated by propagating an ensemble of quantum-classical trajectories with initial conditions obtained by sampling the Wigner distribution of the nuclear degrees of freedom. The electronic coherence of superpositions between the ground and excited and between pairs of excited states is examined for chains of different length, and the dynamics is discussed in terms of the nuclear overlap function that appears in the off-diagonal elements of the electronic reduced density matrix. For long oligomers the loss of coherence occurs in tens of femtoseconds. This time scale is determined by the decay of population into other electronic states through vibronic interactions, and is relatively insensitive to the type and class of superposition considered. By contrast, for smaller oligomers the decoherence time scale depends strongly on the initially selected superposition, with superpositions that can decay as fast as 50 fs and as slow as 250 fs. The long-lived superpositions are such that little population is transferred to other electronic states and for which the vibronic dynamics is relatively harmonic.},
author = {Franco, Ignacio and Brumer, Paul},
doi = {10.1063/1.3700445},
file = {:Users/andyras/Dropbox/mendeley/Franco, Brumer/Franco, Brumer\_The Journal of Chemical Physics\_2012\_Electronic coherence dynamics in trans-polyacetylene oligomers.pdf:pdf},
issn = {1089-7690},
journal = {The Journal of Chemical Physics},
month = apr,
number = {14},
pages = {144501},
pmid = {22502527},
title = {{Electronic coherence dynamics in trans-polyacetylene oligomers.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22502527},
volume = {136},
year = {2012}
}
@article{Ho1996,
abstract = {The mechanisms and the timescales involved in the breaking and making of chemical bonds lie at the foundation of chemical dynamics. Femtosecond laser-induced dynamical quantum processes on solid surfaces (DQPSS) lead to efficient desorption, dissociation, and reaction of adsorbed molecules. Control of the chemical pathways is achieved by varying the duration and fluence of the laser pulses, thus accessing different excitation and energy transfer mechanisms. By adjusting the time delay between two laser pulses, the timescales of DQPSS can be directly probed. The DQPSS driven by femtosecond lasers are also involved in the spatially resolved chemistry and materials modification induced by the tip of a scanning tunneling microscope. Insights into the microscopic mechanisms in conventional thermal desorption and chemistry are also gained from studies of femtosecond laser-induced DQPSS. A general understanding of these different surface phenomena is emerging.},
author = {Ho, Wilson},
journal = {Surface Science},
pages = {166--178},
title = {{Femtosecond laser-induced dynamical quantum processes on solid surfaces (DQPSS)}},
volume = {363},
year = {1996}
}
